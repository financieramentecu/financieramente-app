# Environment variables template for QA environment
# Copy this file to .env and fill in the actual values

# Environment
NODE_ENV=qa

# Database Configuration
POSTGRES_USER=financieramente_user
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=financieramente_qa
DATABASE_URL=postgresql://financieramente_user:your_secure_password_here@postgres:5432/financieramente_qa

# Application Configuration
NEXT_PUBLIC_API_URL=http://localhost:3000

# Optional: Add other environment variables as needed
# JWT_SECRET=your_jwt_secret_here
# ENCRYPTION_KEY=your_encryption_key_here
# SMTP_HOST=smtp.example.com
# SMTP_PORT=587
# SMTP_USER=your_email@example.com
# SMTP_PASS=your_email_password
